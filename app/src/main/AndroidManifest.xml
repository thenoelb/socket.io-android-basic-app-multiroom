<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.noelbehailu.socketio_android_basicapp_multiroom">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name="com.example.noelbehailu.socketio_android_basicapp_multiroom.SocketApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.noelbehailu.socketio_android_basicapp_multiroom.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.noelbehailu.socketio_android_basicapp_multiroom.LoginActivity"
            android:label="@string/title_activity_login">
        </activity>
    </application>

</manifest>